#include <stdio.h>

void main()
{
   int i, n, multiply[9];
   printf("\n1~9의 정수를 입력하세요! : ");
   scanf("%d", &n);
	
   if(n<1)
       printf("\n 1~9의 정수를 입력하세요!!\n");
   else if(n>9)
       printf("\n 1~9의 정수를 입력하세요!!\n");
   else{
	   printf("\n");
	   for(i=0; i<9; i++){
		multiply[i]=n*(i+1);
		printf(" %d * %d = %d \n", n, (i+1), multiply[i]);
	    }		
    }   
   getchar();
}
